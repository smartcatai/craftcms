{
    "name": "smartcat/craft-smartcat-integration",
    "description": "Smartcat Integration plugin for Craft CMS",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "smartcat",
        "integration"
    ],
    "support": {
        "docs": "https://github.com/smartcat/craft-smartcat-integration/blob/master/README.md",
        "issues": "https://github.com/smartcat/craft-smartcat-integration/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Smartcat",
            "homepage": "https://smartcat.com"
        }
    ],
    "require": {
        "craftcms/cms": "^5.3.0"
    },
    "autoload": {
        "psr-4": {
            "smartcat\\smartcatintegration\\": "src/"
        }
    },
    "extra": {
        "name": "Smartcat Integration",
        "handle": "smartcat-integration",
        "developer": "Smartcat",
        "developerUrl": "https://smartcat.com",
        "documentationUrl": "https://github.com/smartcat/craft-smartcat-integration/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/smartcat/craft-smartcat-integration/master/CHANGELOG.md",
        "class": "smartcat\\smartcatintegration\\SmartcatIntegration"
    }
} 